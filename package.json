{
  "name": "storybotgui",
  "productName": "Storybot GUI",
  "version": "0.0.1",
  "description": "Storybut, but with GUI",
  "main": "main.js",
  "scripts": {
    "bundle": "webpack --mode development",
    "serve": "set APP_DEV=true && electron --inspect=5858 .",
    "start": "npm-run-all --parallel bundle serve",
    "build": "webpack --mode production && npm run electron-build",
    "electron-build": "electron-builder",
    "postinstall": "install-app-deps",
    "production_win_x64": "electron-packager . --prune=true --platform=win32 --arch=x64 --out=release-builds",
    "production_win_x32": "electron-packager . --prune=true --platform=win32 --arch=x32 --out=release-builds"
  },
  "author": "Kirill Novak <renato.etc.etc.etc@gmail.com>",
  "repository": "ciricc/storybotgui",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.10",
    "npm-run-all": "^4.1.2",
    "redux-devtools-extension": "^2.13.8",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@vkontakte/icons": "^1.11.3",
    "@vkontakte/mvk-mini-apps-scroll-helper": "^1.1.1",
    "@vkontakte/vk-connect": "^1.5.5",
    "@vkontakte/vkui": "^2.21.5",
    "@vkontakte/vkui-connect": "^1.1.3",
    "@vkontakte/vkui-connect-promise": "^0.1.19",
    "babel-eslint": "^9.0.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "core-js": "^2.6.9",
    "custom-electron-titlebar": "^3.1.0",
    "easyvk": "^2.5.11",
    "electron-log": "^3.0.6",
    "electron-store": "^5.1.0",
    "eruda": "^1.7.2",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "knex": "^0.20.2",
    "minecraft-codes": "0.0.13",
    "nedb": "^1.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.1.2",
    "react-infinite-scroller": "^1.2.4",
    "react-linkify": "^1.0.0-alpha",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.1.0",
    "style-loader": "^0.23.1",
    "url-search-params-polyfill": "^7.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
