{
  "name": "storybotgui",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-renderer-react-scripts-target": "^1.1.1",
    "react-scripts-webpack-config-editor": "^1.0.1"
  },
  "dependencies": {
    "@vkontakte/icons": "^1.4.3",
    "@vkontakte/vkui": "^2.21.5",
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.1.0",
    "history": "^4.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "start": "npm run electron-dev",
    "react-build": "react-scripts build",
    "build": "npm run react-build && npm run electron-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "author": "Kirill Novak",
  "homepage": "./",
  "description": "Storybut, but with GUI"
}
